
<h1>Listing Patients</h1>
 
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
  </tr>
 
  <% @patients.each do |patient| %>
    <tr>
      <td><%= patient.first_Name %></td>
      <td><%= patient.last_Name %></td>
      <td><%= link_to 'Show', patient_path(patient) %></td>
    </tr>
  <% end %>
</table>